{
  "$schema": "https://json.schemastore.org/package",
  "name": "nextjs-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:next": "next dev",
    "dev": "turbo run dev:next",
    "build:next": "next build",
    "build": "turbo run build:next",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "format": "prettier --write \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,cjs,mjs,ts,tsx,md}\"",
    "type:check": "tsc --noEmit"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "clsx": "^2.0.0",
    "dayjs": "^1.11.9",
    "next": "^13.4.19",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "sharp": "^0.32.5",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@testing-library/react": "^14.0.0",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/node": "20.6.2",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "autoprefixer": "10.4.15",
    "eslint": "8.49.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "happy-dom": "^12.0.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "shell-quote": "^1.8.1",
    "tailwindcss": "3.3.3",
    "turbo": "^1.10.14",
    "typescript": "5.2.2",
    "vitest": "^0.34.4"
  },
  "packageManager": "pnpm@8.7.5"
